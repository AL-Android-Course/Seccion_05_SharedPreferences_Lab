<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alejandro.seccion_05_sharedpreferences_lab">

    <application
        android:name="com.alejandro.seccion_05_sharedpreferences_lab.App.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.alejandro.seccion_05_sharedpreferences_lab.Activities.MainActivity" />
        <activity android:name="com.alejandro.seccion_05_sharedpreferences_lab.Activities.LoginActivity" />
        <activity
            android:name="com.alejandro.seccion_05_sharedpreferences_lab.Splash.SplashScreenActivity"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>